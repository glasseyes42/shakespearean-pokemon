{
  "extends": ["airbnb-base", "plugin:import/errors"],
  "rules": {
    "id-length": 0,
    "max-len": 0,
    "no-param-reassign": [2, { "props": false }],
    "consistent-return": 0,
    "arrow-parens": 0,
    "object-curly-newline": ["error", {
      "ObjectPattern": { "multiline": true }
    }],
    "implicit-arrow-linebreak": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "no-underscore-dangle": 0,
    "import/extensions": [
      "error",
      "never",
      {
        "js": "always"
      }
    ]
  }
}
